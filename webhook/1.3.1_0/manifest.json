{
   "background": {
      "page": "bg.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "lgpl/48.png",
      "default_popup": "master_password.html",
      "default_title": "Support Notifications"
   },
   "description": "Notifies about new and updated tickets in Zendesk, GetSatisfaction and Freshdesk",
   "icons": {
      "128": "lgpl/128.png",
      "16": "lgpl/16.png",
      "48": "lgpl/48.png"
   },
   "manifest_version": 2,
   "name": "Support Notifications",
   "options_page": "options.html",
   "permissions": [ "notifications", "http://*.zendesk.com/", "https://*.zendesk.com/", "http://api.getsatisfaction.com/", "http://*.freshdesk.com/", "https://*.freshdesk.com/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.1",
   "web_accessible_resources": [ "lgpl/48.png" ]
}
