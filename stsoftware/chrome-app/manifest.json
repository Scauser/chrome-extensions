{
  "manifest_version": 2,
  "name": "stSoftware App",
  "description": "stSoftware Ltd. App Description",
  "version": "0.0.1",
  "minimum_chrome_version": "42.0.2311.135",
  "icons": {
      "128": "images/icons/128.png",
      "16": "images/icons/16.png"
  },
  "app": {
    "background": {
       "scripts": ["js/background.js", "libs/jquery-2.1.4.js"]
    }
  },
  "permissions": [
    {"fileSystem": ["write", "retainEntries", "directory"]}, "storage", "alarms", "http://*/*", "https://*/*", "*://*/*" 
  ],
  "file_handlers": {
    "text": {
      "types": [
          "*/*"
      ]
    }
  }
}
